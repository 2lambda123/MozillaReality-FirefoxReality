<?xml version='1.0' encoding='UTF-8'?><resources>
    <!-- This string is used on the virtual keyboard to label the enter key. -->
    <string name="keyboard_enter_label">EINGABE</string>
    <!-- This string will be displayed in a dialog box presented to the user
         when the application needs to request permission to access the device's camera.
         '%1$s' will be replaced at runtime with the app name. -->
    <string name="permission_camera">Soll %1$s auf Ihre Kamera zugreifen dürfen?</string>
    <!-- This string will be displayed in a dialog box presented to the user
         when the application needs to request permission to access the device's microphone.
         '%1$s' will be replaced at runtime with the app name. -->
    <string name="permission_microphone">Soll %1$s auf Ihr Mikrofon zugreifen dürfen?</string>
    <!-- This string will be displayed in a dialog box presented to the user
         when the application needs to request permission to access the device's camera and microphone.
         '%1$s' will be replaced at runtime with the app name. -->
    <string name="permission_camera_and_microphone">Soll %1$s auf Ihre Kamera und Ihr Mikrofon zugreifen dürfen?</string>
    <!-- This string will be displayed in a dialog box presented to the user
         when the application needs to request permission to access the device's geographic location.
         '%1$s' will be replaced at runtime with the app name. -->
    <string name="permission_location">Soll %1$s auf Ihren Standort zugreifen dürfen?</string>
    <!-- This string will be displayed in a dialog box presented to the user
         when the application needs to request permission to send system notifications.
         '%1$s' will be replaced at runtime with the app name. -->
    <string name="permission_notification">Soll %1$s Benachrichtigungen versenden dürfen?</string>
    <!-- This string will be displayed in a dialog box presented to the user
         when the application needs to request permission to access the device's external storage such as the
         sdcard. '%1$s' will be replaced at runtime with the app name. -->
    <string name="permission_read_external_storage">Soll %1$s auf Ihren externen Speicher zugreifen dürfen?</string>
    <!-- This string is displayed in the settings dialog box and how
         the user can know which version of the application they are currently running.
         '%1$s' will be replaced at runtime with the application version number. -->
    <string name="settings_version">Version %1$s</string>
    <!-- This string is displayed in the settings dialog and is used to label an
         ON/OFF switch which toggles whether an application crash is reported without first
         prompting the user. -->
    <string name="settings_crash_reporting">Absturzmelder</string>
    <!-- This string is displayed in the settings dialog and is used to label an ON/OFF
         switch which toggles whether telemetry is reported back to Mozilla. -->
    <string name="settings_telemetry">Telemetrie</string>
    <!-- This string is used to label a button that when pressed loads a Web page the
         describes Mozilla's privacy policy. -->
    <string name="settings_privacy">Datenschutzrichtlinie</string>
    <!-- This string is used to label a button that when pressed loads a Web page
         that allows the user file an issue or problem they may have encountered while using the
         application or while visiting a site. -->
    <string name="settings_report_issue">Problem melden</string>
    <!-- This string is used in place of 'settings_version' for developer builds. -->
    <string name="settings_version_developer">Developer Build</string>
    <!-- This string is used to label a button in the settings dialog that when pressed
         opens a dialog box that contains setting an application or Web developer might want to change. -->
    <string name="settings_developer_options">Entwickler-Optionen</string>
    <!-- This string is the title of a dialog box shown to the user when a settings
         change requires the application to restart. -->
    <string name="restart_dialog_restart">Neustart erforderlich</string>
    <!-- This string is the text displayed in the restart dialog box explaining why the
         restart is required. '%1$s' will be replaced at runtime with the app name. -->
    <string name="restart_dialog_text">Sie müssen %1$s neu starten, damit die Änderungen wirksam werden. Möchten Sie dies jetzt tun?</string>
    <!-- This string is used to label the button the user presses to restart the
         application in the restart dialog. -->
    <string name="restart_dialog_button">Jetzt neu starten</string>
    <!-- This string labels an ON/OFF switch in the developer options
         dialog and is used to toggle remote debugging of Web content in the application. -->
    <string name="developer_options_remote_debugging">Externes Debugging aktivieren</string>
    <!-- This string labels an ON/OFF switch in the developer options dialog
         and is used to toggle redirecting Javascript console output to the Android logcat. -->
    <string name="developer_options_show_console">Konsole auf Logcat umleiten</string>
    <!-- This string labels an ON/OFF switch in the developer options dialog
         and is used to toggle loading externally generated environments and overriding the ones built in. -->
    <string name="developer_options_env_override">Übergehen der Umgebung aktivieren</string>
    <!-- This string labels an ON/OFF switch in the developer options dialog
         and is used to toggle running content in a separate process.
         Multiprocessing:
         the running of two or more programs or sequences of instructions simultaneously
         by a computer with more than one central processor. -->
    <string name="developer_options_multiprocess">Mehrfachverarbeitung aktivieren</string>
    <!-- The string an ON/OFF switch in the developer options dialog and is used to toggle enabling servo. -->
    <string name="developer_options_servo">Servo aktivieren</string>
    <!-- This string is used to label a numerical entry field where the user
         may enter a new value to use for the display density. -->
    <string name="developer_options_display_density">Anzeigeauflösung:</string>
    <!-- This string is used to label two integer entry fields where the default height and width of
         the browser window may be set. -->
    <string name="developer_options_window_size">Fenstergröße:</string>
    <!-- This string is used to label buttons to enable editing of text fields in the developer
         options dialog. Pressing this button switches the fields to edit boxes so their values
         may be changed. -->
    <string name="developer_options_edit">Bearbeiten</string>
    <!-- This string is used to label the 'Edit' button once it has been clicked by the user. It
         indicates that when the button is clicked, the user's edits will be saved. -->
    <string name="developer_options_save">Speichern</string>
    <!-- This string is used to label a numerical entry field where the user may enter a new value
         for the virtual displays dots per inch (DPI). -->
    <string name="developer_options_display_dpi">DPI-Wert für Anzeige:</string>
    <!-- This string is used to label two integer entry fields that will allow the user to adjust
         the maximum window size in the X and Y dimensions. -->
    <string name="developer_options_max_window_size">Max. Fenstergröße:</string>
    <!-- This string is used to label radio buttons for setting multi-sample anti-aliasing (MSAA) in
         VR immersive mode. -->
    <string name="developer_options_msaa">MSAA</string>
    <!-- This string is used to label the MSAA radio button that disables MSAA in immersive mode. -->
    <string name="developer_options_msaa_disabled">Deaktiviert</string>
    <!-- This string is used to label the MSAA radio button that enables two times MSAA in immersive mode. -->
    <string name="developer_options_msaa_2">2x</string>
    <!-- This string is used to label the MSAA radio button that enables four times MSAA in immersive mode. -->
    <string name="developer_options_msaa_4">4x</string>
    <!-- This string is used to label a set of radio buttons that allow the user to
     change the user-agent (UA) string of the browser. -->
    <string name="developer_options_ua_mode">User-Agent-Modus</string>
    <!-- This string is used to label the desktop user-agent radio button of the 'User-Agent Mode'
         settings option. -->
    <string name="developer_options_ua_desktop">Desktop</string>
    <!-- This string is used to label the mobile user-agent radio button of the 'User-Agent Mode'
         settings option. -->
    <string name="developer_options_ua_mobile">Mobil</string>
    <!-- This string is used to label the virtual reality (VR) user-agent radio button of the
         'User-Agent Mode' settings option. -->
    <string name="developer_options_ua_vr">VR</string>
    <!-- This string describes what the 'Reset' button in the developer options does which is
         restore all the developer settings to their default value. -->
    <string name="developer_options_reset">Entwicklereinstellungen zurücksetzen</string>
    <!-- This string labels the 'Reset' button that restores the default developer settings values.  -->
    <string name="developer_options_reset_button">Zurücksetzen</string>
    <!-- This string describes the radio buttons used to select the environment seen while not in
         immersive mode. -->
    <string name="developer_options_environments">Umgebung</string>
    <!-- This string labels the radio button that selects the 'Meadow' environment. -->
    <string name="developer_options_env_meadow">Wiese</string>
    <!-- This string labels the radio button that selects the 'Cave' environment. -->
    <string name="developer_options_env_cave">Höhle</string>
    <!-- This string labels the radio button that selects the 'Void' environment. The void
         environment is an empty space that surrounds the user in empty, black space. -->
    <string name="developer_options_env_void">Leere</string>
    <!-- This string describes the radio buttons used to select the pointer color. -->
    <string name="developer_options_pointer_color">Zeigerfarbe</string>
    <!-- This string labels the radio button that sets the pointer color to white. -->
    <string name="developer_options_pointer_white">Weiß</string>
    <!-- This string labels the radio button that sets the pointer color to purple.  -->
    <string name="developer_options_pointer_purple">Purpur</string>
    <!-- This string is displayed below the ON/OFF switch to indicated the switches current
         state as 'ON'.  -->
    <string name="on">Ein</string>
    <!-- This string is displayed below the ON/OFF switch to indicated the switches current
         state as 'OFF'.  -->
    <string name="off">Aus</string>
    <!-- This string is displayed in the voice search dialog box that is show to the user when they press the
         button used to initiate voice search. -->
    <string name="voice_search_start">Wonach möchten Sie im Web suchen?</string>
    <!-- This string is displayed in the voice search dialog box and gives examples of phrases they
         might use in a voice search. -->
    <string name="voice_search_example">Beispiel: Bitte sagen Sie: \"360 Videos, Wetter, Nachrichten…\"</string>
    <!-- This string is displayed in a dialog box show to the user while the voice search is being
         performed. It indicates that the search query is running and they should wait for the result. -->
    <string name="voice_search_decoding">Suchen…</string>
    <!-- This string is displayed in a dialog box when the phrase the user spoke for the voice search
         was not understood by the system. -->
    <string name="voice_search_error">Tut mir leid! Das habe ich nicht richtig verstanden.</string>
    <!-- This string is displayed in a dialog box after a failed voice search and is prompting the
         user to try the voice search again. -->
    <string name="voice_search_try_again">Bitte versuchen Sie es erneut.</string>
    <!-- This string is displayed in a dialog after the user has declined permission to use the
         devices microphone. It explains why voice search is not working. -->
    <string name="voice_search_permission_after_decline">Bitte erlauben Sie den Zugriff auf das Mikrofon, um die Sprachsuche zu verwenden.</string>
    <!-- This string is displayed in the URL bar when it is empty. It informs the user that they
         may use the URL bar to search the Web or enter a Web address. -->
    <string name="search_placeholder">Das Web durchsuchen oder eine Adresse eingeben</string>
    <!-- This string labels a button used to cancel an action. -->
    <string name="cancel_button">Abbrechen</string>
    <!-- This string labels a button that are used to approve an action. -->
    <string name="ok_button">OK</string>
    <!-- This string labels a button to go back to the previous menu or exits the current mode -->
    <string name="back_button">Zurück</string>
    <!-- This string is displayed in a dialog after an application crash. -->
    <string name="crash_dialog_heading">Leider hat ein Problem zu einem Absturz geführt.</string>
    <!-- This string explains why Mozilla would like the user to allow the crash data to be uploaded.
         '%1$s' will be replaced at runtime with the app name. -->
    <string name="crash_dialog_message">Bitte helfen Sie Mozilla, %1$s zu verbessern, indem Sie uns die Absturzdaten senden.</string>
    <!-- This string labels a checkbox in the crash dialog box. When checked, the user will no longer
         be prompted for permission to upload crash data after an application crash. -->
    <string name="crash_dialog_send_data">Daten immer ohne Rückfrage senden.</string>
    <!-- This string labels a button that when clicked allows the user to learn more about the
         currently open dialog. -->
    <string name="learn_more_button">Weitere Informationen</string>
    <!-- This string labels a button that when clicked prevents crash data from being sent to Mozilla. -->
    <string name="do_not_sent_button">Nicht senden</string>
    <!-- This string labels a button that when clicked allows the crash data to be sent to Mozilla. -->
    <string name="send_data_button">Daten senden</string>
    <!-- This string labels a button to open the Brightness mode selector-->
    <string name="brightness_mode_tooltip">Helligkeit</string>
    <!-- This string is displayed when selecting a background brightness mode -->
    <string name="brightness_mode_normal">Normal</string>
    <!-- This string is displayed when selecting a background brightness mode -->
    <string name="brightness_mode_dark">Dunkel</string>
    <!-- This string is displayed when selecting a background brightness mode -->
    <string name="brightness_mode_void">Leere</string>
    <!-- This string labels a button to open the Video projection mode selector-->
    <string name="video_mode_tooltip">Videoprojektion</string>
    <!-- This string is displayed when selecting a video playback mode -->
    <string name="video_mode_3d_side">3D nebeneinander</string>
    <!-- This string is displayed when selecting a video playback mode -->
    <string name="video_mode_360">360</string>
    <!-- This string is displayed when selecting a video playback mode -->
    <string name="video_mode_360_stereo">360 Stereo</string>
    <!-- This string is displayed when selecting a video playback mode -->
    <string name="video_mode_180">180</string>
    <!-- This string is displayed when selecting a video playback mode -->
    <string name="video_mode_180_left_right">Stereo 180 (links/rechts)</string>
    <!-- This string is displayed when selecting a video playback mode -->
    <string name="video_mode_180_top_bottom">Stereo 180 (oben/unten)</string>
    <!-- This string is displayed when selecting a video playback mode -->
    <string name="video_mode_2d">2D</string>
    <!-- This string is displayed as a tooltip in the video controls UI -->
    <string name="video_controls_play">Wiedergabe</string>
    <!-- This string is displayed as a tooltip in the video controls UI -->
    <string name="video_controls_pause">Pause</string>
    <!-- This string is displayed as a tooltip in the video controls UI -->
    <string name="video_controls_mute">Ton aus</string>
    <!-- This string is displayed as a tooltip in the video controls UI -->
    <string name="video_controls_unmute">Ton an</string>
    <!-- This string is displayed as a tooltip in the video controls UI -->
    <string name="video_controls_volume">Lautstärke</string>
    <!-- This string is displayed as a tooltip in the video controls UI. E.g Seek +10 seconds -->
    <string name="video_controls_seek_forward">Suchen: +%1$ Sekunden</string>
    <!-- This string is displayed as a tooltip in the video controls UI. E.g Seek -10 seconds -->
    <string name="video_controls_seek_backward">Suchen: -%1$ Sekunden</string>
</resources>
